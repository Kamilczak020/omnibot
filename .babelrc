{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current",
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-for-of",
    "@babel/plugin-transform-regenerator",
    "@babel/plugin-transform-destructuring"
  ]
}